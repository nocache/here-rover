<%= javascript_tag do %>
  var kml_url = '<%= rover_url('all') %>';
<% end %>
<div id="map3d"></div>
<div id="chrome-for-mac" style="display:none" class="alert alert-danger alert-dismissible" role="alert">
  <button type="button" class="close" data-dismiss="alert">
    <span aria-hidden="true">&times;</span><span class="sr-only">Close</span>
  </button>
  The Google Earth plugin is no longer supported in Chrome for Mac OSX,
  consider switching to Safari for this site.
</div>
<div id="ge-plugin-error" style="display:none" class="alert alert-danger alert-dismissible" role="alert">
  <button type="button" class="close" data-dismiss="alert">
    <span aria-hidden="true">&times;</span><span class="sr-only">Close</span>
  </button>
  The Google Earth plugin did not load. Sorry.
</div>
<div id="alt-content" style="display:none"></div>
